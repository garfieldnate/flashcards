{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start-verbose": "EXPO_DEBUG=1 DEBUG=pouchdb:* expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "node_modules/.bin/jest",
    "test:debug": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand",
    "lint": "tslint -p tsconfig.json ./*.{ts,tsx} src/**/*.{ts,tsx}",
    "fix-swiper": "patch -p0 <patches/swiper.patch",
    "fix-util": "patch -p0 <patches/util.patch",
    "fix-get_attachment": "patch -p0 <patches/get_attachment.patch",
    "format": "prettier --write *.ts src/**/*.{ts,tsx}",
    "postinstall": "npm run fix-swiper && npm run fix-util && npm run fix-get_attachment",
    "schemata2ts": "ts-node ./scripts/schemata2ts.ts"
  },
  "dependencies": {
    "//": "Important: adapters should all be 7.0.0 to match RxDB",
    "@expo/vector-icons": "^10.0.3",
    "expo": "^34.0.3",
    "expo-av": "^6.0.0",
    "mobx": "^5.10.1",
    "mobx-react": "^6.1.1",
    "mobx-utils": "^5.4.1",
    "moment": "^2.24.0",
    "native-base": "^2.12.1",
    "neverthrow": "^1.0.2",
    "pouchdb-adapter-asyncstorage": "7.0.0-beta-1",
    "pouchdb-adapter-http": "7.0.0",
    "pouchdb-debug": "^7.1.1",
    "react": "16.8.3",
    "react-dom": "^16.8.6",
    "react-native": "https://github.com/expo/react-native/archive/sdk-34.0.0.tar.gz",
    "react-native-card-flip": "^1.0.4",
    "react-native-deck-swiper": "1.6.7",
    "react-native-gesture-handler": "~1.3.0",
    "react-native-modal": "^11.0.2",
    "react-native-reanimated": "~1.1.0",
    "react-native-view-overflow": "0.0.4",
    "react-native-web": "^0.11.4",
    "react-navigation": "^3.11.1",
    "rxdb": "^8.2.0",
    "rxjs": "^6.5.2",
    "shuffle-array": "^1.0.1",
    "typescript-optional": "^2.0.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/promise": "^7.1.30",
    "@types/react": "^16.8.19",
    "@types/react-native": "^0.57.60",
    "@types/react-navigation": "^3.0.7",
    "@types/react-test-renderer": "^16.8.2",
    "@types/shuffle-array": "^0.0.28",
    "add": "^2.0.6",
    "babel-preset-expo": "^5.2.0",
    "expo-cli": "^3.0.6",
    "jest-expo": "^34.0.0",
    "json-schema-to-typescript": "^6.1.3",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.5",
    "yarn": "^1.16.0"
  },
